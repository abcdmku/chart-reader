{
  "name": "@chart-reader/server",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts --host 127.0.0.1",
    "db:introspect": "drizzle-kit introspect --config=drizzle.config.ts",
    "csv:rebuild": "tsx scripts/rebuild-csv.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "ai": "^4.0.0",
    "express": "^4.19.2",
    "mime-types": "^2.1.35",
    "multer": "^2.0.2",
    "nanoid": "^5.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@libsql/client": "^0.17.0",
    "@types/express": "^4.17.21",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.0.0",
    "dotenv": "^17.3.1",
    "drizzle-kit": "^0.31.9",
    "drizzle-orm": "^0.45.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  }
}
